@base <https://w3id.org/unterrichtsvideos.net/vocabs/resource_type/> .
@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vann: <http://purl.org/vocab/vann/> .

<> a skos:ConceptScheme;
    dct:title "Ressourcentypen"@de ;
    dct:title "Ressource types"@en ;
    dct:description "Eine Wertelliste für Ressourcentypen"@de, "A value list for ressource types"@en ;
    dct:publisher <https://unterrichtsvideos.net> ;
    dct:created "2022-06-01"^^xsd:date ;
    dct:modified "2025-07-20"^^xsd:date ;
    dct:issued "2022-06-01"^^xsd:date ;
    vann:preferredNamespaceUri "https://w3id.org/unterrichtsvideos.net/vocabs/resource_type/" ;
    vann:preferredNamespacePrefix "mvpResourceType" ;
    dct:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    skos:hasTopConcept <rt005>, <rt004>, <rt002>, <rt000>, <rt003>, <rt007>, <rt008>, <rt009>, <rt001>, <rt006>, <rt010>, <rt011>, <rt012> .

<rt005> a skos:Concept ;
    skos:prefLabel "Authentischer Unterricht"@de ;
    skos:prefLabel "Authentic classroom video"@en ;
    skos:definition "Ein Video, das eine reale Unterrichtssituation dokumentiert."@de ;
    skos:definition "A video that documents a real classroom situation."@en ;
    skos:topConceptOf <> .

<rt004> a skos:Concept ;
    skos:prefLabel "Inszenierter Unterricht"@de ;
    skos:prefLabel "Staged classroom video"@en ;
    skos:definition "Ein Video, das eine (geskriptete) Unterrichtssituation darstellt, die für die Aufnahme inszeniert wurde."@de ;
    skos:definition "A video that depicts a (scripted) classroom situation that was staged for the recording."@en ;
    skos:topConceptOf <> .

<rt002> a skos:Concept ;
    skos:prefLabel "Animierter Unterricht"@de ;
    skos:prefLabel "Animated classroom video"@en ;
    skos:definition "Ein Video, das eine animierte Unterrichtssituation darstellt, bspw. animierter Unterricht oder (VR-)Simulationen."@de ;
    skos:definition "A video that depicts an animated classroom situation, e.g. animated teaching or (VR) simulations."@en ;
    skos:topConceptOf <> .

<rt000> a skos:Concept ;
    skos:prefLabel "Sonstiger Unterricht"@de ;
    skos:prefLabel "Other classroom video"@en ;
    skos:definition "Ein Video, das eine Unterrichtssituation darstellt, die nicht als inszenierter/animierter oder authentischer Unterricht kategorisiert werden kann."@de ;
    skos:definition "A video that depicts a classroom situation that cannot be categorized as either staged/animated or authentic teaching."@en ;
    skos:topConceptOf <> .

<rt003> a skos:Concept ;
    skos:prefLabel "Interview"@de ;
    skos:prefLabel "Interview"@en ;
    skos:definition "Inhalte, die ein Gespräch oder Interview mit einer oder mehreren Personen zeigen, z. B. einer Lehrkraft."@de ;
    skos:definition "Content that features a conversation or interview with one or more persons, e.g. a teacher."@en ;
    skos:topConceptOf <> .

<rt007> a skos:Concept ;
    skos:prefLabel "Lernvideo"@de ;
    skos:prefLabel "Learning video"@en ;
    skos:definition "Ein Video, das Lerninhalte vermittelt, z. B. Erklärvideos, Tutorials oder Vorträge."@de ;
    skos:definition "A video that conveys learning content, e.g. explainer videos, tutorials, or lectures."@en ;
    skos:topConceptOf <> .

<rt008> a skos:Concept ;
    skos:prefLabel "Lernmodul"@de ;
    skos:prefLabel "Learning module"@en ;
    skos:definition "Ein Bildungsangebot, das spezifische Lernziele zu einem bestimmten Thema umfasst."@de ;
    skos:definition "An educational resource that encompasses specific learning objectives on a particular topic."@en ;
    skos:topConceptOf <> .

<rt009> a skos:Concept ;
    skos:prefLabel "Learning Nugget"@de ;
    skos:prefLabel "Learning Nugget"@en ;
    skos:definition "Ein kurzes, fokussiertes Bildungsangebot, das ein spezifisches Thema oder Lernziel behandelt."@de ;
    skos:definition "A short, focused educational resource that addresses a specific topic."@en ;
    skos:note "Dieser Typ steht zur Diskussion, da redundant zur Erfassung der benötigten Zeit."@de ;
    skos:note "This type is under discussion, as it is redundant for capturing the required time."@en ;
    skos:topConceptOf <> .

<rt001> a skos:Concept ;
    skos:prefLabel "Sonstiges Bildungsangebot"@de ;
    skos:prefLabel "Other educational resource"@en ;
    skos:topConceptOf <> .

<rt006> a skos:Concept ;
    skos:prefLabel "Lehrfilm"@de ;
    skos:prefLabel "Teaching video"@en ;
    owl:deprecated true ;
    skos:note "Zusammengeführt mit Lernvideo."@de, "Merged concept with Learning video."@en ;
    dct:isReplacedBy <rt007> ;
    skos:topConceptOf <> .

<rt010> a skos:Concept ;
    skos:prefLabel "Unterrichtsstunde"@de ;
    skos:prefLabel "Teaching session"@en ;
    skos:definition "Ein Unterrichtsvideo, das eine vollständige Unterrichtsstunde umfasst."@de ;
    skos:definition "A classroom video that encompasses a complete teaching session."@en ;
    owl:deprecated true ;
    skos:note "Entfällt, da redundant zur Erfassung der benötigten Zeit."@de, "Discontinued, as it is redundant for capturing the required time."@en ;
    skos:topConceptOf <> .

<rt011> a skos:Concept ;
    skos:prefLabel "Unterrichtsausschnitt"@de ;
    skos:prefLabel "Teaching segment"@en ;
    skos:definition "Ein Unterrichtsvideo, das einen Ausschnitt aus einer Unterrichtsstunde darstellt."@de ;
    skos:definition "A classroom video that represents a segment of a teaching session."@en ;
    owl:deprecated true ;
    skos:note "Entfällt, da redundant zur Erfassung der benötigten Zeit."@de, "Discontinued, as it is redundant for capturing the required time."@en ;
    skos:topConceptOf <> .

<rt012> a skos:Concept ;
    skos:prefLabel "Unterrichtsreihe"@de ;
    skos:prefLabel "Teaching series"@en ;
    owl:deprecated true ;
    skos:note "Entfällt vorerst."@de, "Is temporarily discontinued."@en ;
    skos:topConceptOf <> .

